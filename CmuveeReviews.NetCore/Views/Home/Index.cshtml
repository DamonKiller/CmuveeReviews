                                <article class="col-lg-3 col-md-6 col-sm-4">
                                    <!-- POST L size -->
                                    <div class="post post-medium">
                                        <div class="thumbr">
                                            <a class="afterglow post-thumb" href="@item.PlayAddress.Split(new char[] {'$' },StringSplitOptions.RemoveEmptyEntries)[2]" data-lity target="_blank">
                                                <span class="play-btn-border" title="Play"><i class="fa fa-play-circle headline-round" aria-hidden="true"></i></span>
                                                @*<div class="cactus-note ct-time font-size-1"><span>02:02</span></div>*@
                                                <img class="img-responsive" src="@item.PicsAddress.Replace("<img src=\"","").Replace("\"/>","")" alt="#">
                                            </a>
                                        </div>
                                        <div class="infor">
                                            <h4>
                                                <a class="title" href="#">@item.Name</a>
                                            </h4>
                                            <span class="posts-txt" title="Posts from Channel"><i class="fa fa-thumbs-up" aria-hidden="true"></i>@item.DoubanScore</span>
